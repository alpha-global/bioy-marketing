{
  "name": "bioy-marketing-site",
  "version": "0.0.1",
  "description": "Marketing site for Bible in One Year",
  "scripts": {
    "11ty": "ELEVENTY_ENV=development npx @11ty/eleventy --serve",
    "watch": "mix watch",
    "dev": "ELEVENTY_ENV=development npx @11ty/eleventy --incremental --serve && mix watch",
    "dev:cleanSlate": "npm run wipe && ELEVENTY_ENV=development CLEAN_SLATE=true npx @11ty/eleventy",
    "cms": "npx netlify-cms-proxy-server",
    "wipe": "rm -rf ./.cache && rm -rf ./build",
    "production": "ELEVENTY_ENV=production npx @11ty/eleventy && mix --production",
    "production:cleanSlate": "ELEVENTY_ENV=production CLEAN_SLATE=true npx @11ty/eleventy && mix --production"
  },
  "author": "Alpha International / OneSheep",
  "license": "ISC",
  "dependencies": {
    "alpinejs": "^3.8.1",
    "swipejs": "^2.3.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-cache-assets": "^2.2.1",
    "@11ty/eleventy-img": "^0.10.0",
    "@ayctor/laravel-mix-svg-sprite": "^1.0.0",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.2",
    "dayjs": "^1.10.5",
    "laravel-mix": "^6.0.41",
    "postcss": "^8.4.5",
    "slugify": "^1.5.3",
    "svg-spritemap-webpack-plugin": "^3.9.1",
    "tailwindcss": "^3.0.15"
  }
}
