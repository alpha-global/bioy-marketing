{
  "name": "bioy-marketing-site",
  "version": "0.0.1",
  "description": "Marketing site for Bible in One Year",
  "scripts": {
    "dev": "ELEVENTY_ENV=development eleventy --serve & mix watch",
    "11ty": "ELEVENTY_ENV=development eleventy --serve",
    "dev:cleanSlate": "npm run wipe && ELEVENTY_ENV=development CLEAN_SLATE=true eleventy",
    "cms": "npx netlify-cms-proxy-server",
    "wipe": "rm -rf ./.cache && rm -rf ./build",
    "production": "ELEVENTY_ENV=production eleventy && mix --production",
    "production:cleanSlate": "ELEVENTY_ENV=production CLEAN_SLATE=true eleventy&& mix --production"
  },
  "author": "Alpha International / OneSheep",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-cache-assets": "^2.2.1",
    "@11ty/eleventy-img": "^0.9.0",
    "@ayctor/laravel-mix-svg-sprite": "^1.0.0",
    "alpinejs": "^2.8.2",
    "laravel-mix": "^6.0.41",
    "slugify": "^1.5.3",
    "svg-spritemap-webpack-plugin": "^3.9.1",
    "swipejs": "^2.3.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.0",
    "dayjs": "^1.10.5",
    "tailwindcss": "^3.0.0"
  }
}
